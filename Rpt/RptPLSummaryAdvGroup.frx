    Dim  Total, Total2 as Double   
    
    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)        
      Total = 0
      Total2 = 0    
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Total = Total + CType(Report.GetColumnValue(&quot;DS.Total&quot;), Double) 
      Total2 = Total2 + CType(Report.GetColumnValue(&quot;DS.Total2&quot;), Double) 
    End Sub

    Private Sub GroupFooter1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Total = Math.Round(Total,0)
      total2 = Math.Round(Total2,0)
      if (Total - Total2 = 0)Or(Total2 = 0) then 
        Text71.Text = &quot;100.00&quot;
      else 
        Text71.Text = FormatNumber( (total - total2) * 100/ total2, 2)
      end if 
      'Text43.Text = FormatNumber(Abs(Total), 0)
      'Text42.Text = FormatNumber(Abs(Total2), 0)
    End Sub

    Private Sub GroupFooter2_AfterData(ByVal sender As object, ByVal e As EventArgs)
      'Text52.Text = FormatNumber(CType(Report.GetColumnValue(&quot;DS.PLTotal&quot;), Double) * CType(Report.GetColumnValue(&quot;DS.FgTotal&quot;), Int32),0)
      'Text51.Text = FormatNumber(CType(Report.GetColumnValue(&quot;DS.PLTotal2&quot;), Double) * CType(Report.GetColumnValue(&quot;DS.FgTotal2&quot;), Int32),0) 
    End Sub
