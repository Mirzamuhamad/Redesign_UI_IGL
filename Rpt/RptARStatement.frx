    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Debet = 0  
      Kredit = 0  
      Saldo = CType(Report.GetColumnValue(&quot;DS.TotalBegin&quot;), Decimal) 
      SaldoDPP = CType(Report.GetColumnValue(&quot;DS.TotalBeginBase&quot;), Decimal)
      SaldoPPnHome = CType(Report.GetColumnValue(&quot;DS.TotalBeginPPnHome&quot;), Decimal)
    End Sub
    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      'Saldo = Saldo + CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)
      Saldo = Saldo + CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)
      SaldoDPP = SaldoDPP + CType(Report.GetColumnValue(&quot;DS.BaseDr&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.BaseCr&quot;), Decimal)
      SaldoPPnHome = SaldoPPnHome + CType(Report.GetColumnValue(&quot;DS.PPnDrHome&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.PPnCrHome&quot;), Decimal)
      Debet = Debet + CType(Report.GetColumnValue(&quot;DS.BaseDr&quot;), Decimal)
      Kredit = Kredit + CType(Report.GetColumnValue(&quot;DS.BaseCr&quot;), Decimal)
      'Text43.Text = FormatNumber(Saldo,0)
      Text43.Text = FormatNumber(SaldoDPP,2)
      txtSaldoPPnhome.Text = FormatNumber(SaldoPPnHome,2)
      Text66.Visible= True 
      if CType(Report.GetColumnValue(&quot;DS.Document&quot;), String) = &quot;&quot; then Text66.Visible = false
      
      
    End Sub

    Private Sub GroupFooter1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Text44.Text = FormatNumber(Debet,2)
      Text45.Text = FormatNumber(Kredit,2)
      Text46.Text = FormatNumber(SaldoDPP,2)  
      txtTotalSaldoPPnhome.Text = FormatNumber(SaldoPPnHome,2)
    End Sub
  End Class
