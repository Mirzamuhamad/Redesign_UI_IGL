    dim supp as String
    dim No as Integer
    Dim Count As Integer = 0
    
    Private Sub ReportTitle1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
        supp = &quot;&quot;
        No = 0
        if Report.GetColumnValue(&quot;DS.FgForce&quot;) = &quot;Y&quot; then
          GroupHeader1.StartNewPage = true
        end if
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      supp = &quot;&quot;
    End Sub

    Private Sub GroupHeader2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      supp = &quot;&quot;
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Count = Count + 1
      if supp &lt;&gt; Report.GetColumnValue(&quot;DS.Customer_Code&quot;) then
        supp = Report.GetColumnValue(&quot;DS.Customer_Code&quot;)
        No = No + 1
        txtNo.Text = No.ToString
        txtSupp.Visible=true
        txtSuppName.Visible = true
        txtNo.Visible = true            
      else
        txtSupp.Visible=false
        txtSuppName.Visible = false
        txtNo.Visible = false             
      end if
    End Sub

    Private Sub GroupHeader3_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Count = 0
    End Sub

    Private Sub GroupFooter3_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      If Count = 1 Then
        'GroupFooter2.Height = 0
        Text61.Visible = False
        Text62.Visible = False
        Text63.Visible = False
        Text64.Visible = False
        Text65.Visible = False  
        Text66.Visible = False  
        Line4.Visible = False
      Else 
        'GroupFooter2.Height = 0.5
        Text61.Visible = True
        Text62.Visible = True
        Text63.Visible = True
        Text64.Visible = True
        Text65.Visible = True   
        Text66.Visible = True
        Line4.Visible = True 
      End If
    End Sub
  End Class
End Namespace
