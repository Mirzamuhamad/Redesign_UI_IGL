        Text38.Visible = False
      Else 
        Text21.Visible = True
        Text43.Visible = True
        Text44.Visible = True
        Text38.Visible = True
      End If
    End Sub

    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
       If CType(Report.GetColumnValue(&quot;DS.FgForce&quot;), Int32) = 0 Then
          GroupHeader1.StartNewPage = False
       Else : GroupHeader1.StartNewPage = True
       End If 
    End Sub

    Private Sub Data1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
       TransNmbr = CType(Report.GetColumnValue(&quot;DS.TransNo&quot;), String)
    End Sub
  End Class
