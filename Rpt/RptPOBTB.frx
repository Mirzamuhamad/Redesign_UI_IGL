    Private Sub GroupHeader2_AfterData(ByVal sender As object, ByVal e As EventArgs)
      QtyOut = CType(Report.GetColumnValue(&quot;DS.QtyPO&quot;), Decimal)
      QtyOutOrder = CType(Report.GetColumnValue(&quot;DS.QtyPO2&quot;), Decimal)
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      txtWrhsOut.Text = FormatNumber(QtyOut -  CType(Report.GetColumnValue(&quot;DS.QtyRR&quot;), Decimal),0)
      txtOrderOut.Text = FormatNumber(QtyOutOrder -  CType(Report.GetColumnValue(&quot;DS.QtyRR2&quot;), Decimal),0)
    End Sub

