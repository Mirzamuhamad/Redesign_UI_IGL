    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      i = i + 1
      GroupFooter3.Visible = False
    End Sub

    Private Sub GroupFooter3_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      GroupFooter3.Visible = i &gt; 1 
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Grup = &quot;&quot;
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Grup = CType(Report.GetColumnValue(&quot;DS.TypeCode&quot;), String)
    End Sub

    Private Sub ColumnHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      IF Grup = CType(Report.GetColumnValue(&quot;DS.TypeCode&quot;), String) Then 
          ColumnHeader1.Visible = True
      Else
          ColumnHeader1.Visible = False
      End If
    End Sub
  End Class
