    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
    If CType(Report.GetColumnValue(&quot;DS.FgSchedule&quot;), String) = &quot;Y&quot; then
        sTrainingA.Visible = True
        sTrainingB.Visible = False         
    Else
        sTrainingA.Visible = False
        sTrainingB.Visible = True         
    End If

    If CType(Report.GetColumnValue(&quot;DS.FgSertifikat&quot;), String) = &quot;YA&quot; then
        sSertifikatA.Visible = True
        sSertifikatB.Visible = False         
    Else
        sSertifikatA.Visible = False
        sSertifikatB.Visible = True         
    End If
    End Sub
  

    Private Sub MasterData2_AfterData(ByVal sender As object, ByVal e As EventArgs)
