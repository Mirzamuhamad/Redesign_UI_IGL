    Dim Nomor As Integer = 0
    Dim Count As Integer = 0
    Dim TransNmbr As String = &quot;&quot;
    
    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Nomor = 0
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      
      Count = Count + 1
      
      If CType(Report.GetColumnValue(&quot;DS.TransNo&quot;), String) = TransNmbr Then
        Text5.Visible = False
        Text1.Visible = False
      Else 
        Text5.Visible = True
        Nomor = Nomor + 1
        Text1.Text = Nomor.ToString
        Text1.Visible = True
      End If
      
      Text3.Visible = Text5.Visible
      Text7.Visible = Text5.Visible
    End Sub

    Private Sub GroupHeader2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Count = 0
      TransNmbr = &quot;&quot;
    End Sub

    Private Sub GroupFooter2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      If Count = 1 Then
        'GroupFooter2.Height = 0
        Text21.Visible = False
        Text22.Visible = False
        Text34.Visible = False
        Text35.Visible = False
        Text38.Visible = False
        Text10.Visible = False
      Else 
        'GroupFooter2.Height = 0.5
        Text21.Visible = True
        Text22.Visible = True
        Text34.Visible = True
        Text35.Visible = True
        Text38.Visible = True
         Text10.Visible = True
      End If
    End Sub

    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      If CType(Report.GetColumnValue(&quot;DS.FgForce&quot;), Int32) = 0 Then
        GroupHeader1.StartNewPage = False
      Else : GroupHeader1.StartNewPage = True
      End If
    End Sub

    Private Sub Data1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      TransNmbr = CType(Report.GetColumnValue(&quot;DS.TransNo&quot;), String)
    End Sub
  End Class
End Namespace
