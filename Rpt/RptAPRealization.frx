  Public Class ReportScript
    dim Invois as String
    dim Sel, TInvois, TCurr as double

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      if Report.GetColumnValue(&quot;DS.PayDate&quot;) = &quot;01 jan 0001&quot;  then
        Text13.Visible = false                      
      else
        Text13.Visible = true             
      End If  
      
      if Invois &lt;&gt; Report.GetColumnValue(&quot;DS.InvoiceNo&quot;) then
        Invois = Report.GetColumnValue(&quot;DS.InvoiceNo&quot;)
        Sel = CDbl(Report.GetColumnValue(&quot;DS.AmountInvoice&quot;)) - Cdbl(Report.GetColumnValue(&quot;DS.AmountPay&quot;)) 
        TInvois = TInvois + CDbl(Report.GetColumnValue(&quot;DS.AmountInvoice&quot;))
        TCurr = TCurr + CDbl(Report.GetColumnValue(&quot;DS.AmountInvoice&quot;))
      else
        sel = Sel - Cdbl(Report.GetColumnValue(&quot;DS.AmountPay&quot;)) 
      End If
      TxtSel.Text = FormatNumber(Sel, 0)  
    End Sub

    Private Sub ReportTitle1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Invois = &quot;&quot;
      sel = 0
      TInvois = 0
      TCurr = 0
    End Sub

    Private Sub GroupHeader2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      TInvois = 0      
    End Sub    

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      TCurr = 0
    End Sub

    Private Sub GroupFooter2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      txtTInv.Text = FormatNumber(TInvois, 0)
    End Sub

    Private Sub txtTCurr_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      txtTCurr.Text =  FormatNumber(TCurr, 0)
    End Sub
  End Class
End Namespace
</ScriptText>
