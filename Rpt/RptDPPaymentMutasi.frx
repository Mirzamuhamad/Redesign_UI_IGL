    Dim SAkhir, SAwal, SDebet, SKredit AS Double

    Private Sub Page1_StartPage(ByVal sender As object, ByVal e As EventArgs)
      If  CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 0 then        
        Text20.Text = &quot;Laporan Mutasi Advance&quot; 
        Text2.Text = &quot;No. Dok&quot;
        Text6.Text = &quot;No. Giro&quot;
        Text8.Text = &quot;Keterangan&quot;
        Text10.Text = &quot;User&quot;
      ElseIf CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 1 Then        
        Text20.Text = &quot;Laporan Mutasi Advance with PO&quot; 
        Text2.Text = &quot;No. Dok&quot;
        Text6.Text = &quot;No. Giro&quot;
        Text8.Text = &quot;No. PO&quot;
        Text10.Text = &quot;Supplier&quot;
      Else
        Text20.Text = &quot;Laporan Mutasi Advance without PO&quot; 
        Text2.Text = &quot;Reff BG/Ch&quot;
        Text6.Text = &quot;No. Voucher&quot;
        Text8.Text = &quot;Keterangan&quot;
        Text10.Text = &quot;User&quot;
      End if         
    End Sub

    Private Sub GroupHeader1_AfterData(ByVal sender As object, ByVal e As EventArgs)
        SAkhir = 0
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
        SAwal = CType(Report.GetColumnValue(&quot;DS.TotalBegin&quot;), Double)
        SDebet = CType(Report.GetColumnValue(&quot;DS.TotalDr&quot;), Double)
        SKredit = CType(Report.GetColumnValue(&quot;DS.TotalCr&quot;), Double)
        SAkhir = SAkhir + SAwal + SDebet - SKredit 
        Text17.Text = FormatNumber(SAkhir,0) 
    End Sub

    Private Sub GroupFooter1_AfterData(ByVal sender As object, ByVal e As EventArgs)
        Text23.Text = FormatNumber(SAkhir,0)
    End Sub
  End Class
End Namespace
</ScriptText>
