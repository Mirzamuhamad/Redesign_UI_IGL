    Dim No As Integer
    Dim StrQreGroup As String

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
       No = 0
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      If (No=0) Then   
        StrQreGroup = CType(Report.GetColumnValue(&quot;DS.GroupName&quot;), String)  
      End If
      If (CType(Report.GetColumnValue(&quot;DS.GroupName&quot;), String) &lt;&gt; StrQreGroup) Then 
          GroupHeader1.StartNewPage = True
      End If
      
      StrQreGroup = CType(Report.GetColumnValue(&quot;DS.GroupName&quot;), String)
      No = No+1
      Text21.Text = No.ToString
    End Sub

    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      If (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 0) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 7) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 14) Then
        qrlJudulReport2.Text = &quot;Employee Under Contract Report&quot;
        qrlSortBy2.Text = &quot;Report Type : Under Contract Per Employee&quot;
        ChildBand2.Visible = False
      Else
        qreGroup.Text = CType(Report.GetColumnValue(&quot;DS.GroupName&quot;), String)
        ChildBand2.Visible = True
        If (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 1) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 8) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 15) Then
         qrlJudulReport2.Text = &quot;Employee Under Contract Report&quot;
         qrlSortBy2.Text = &quot; Report Type : Under Contract Per Department&quot;
         qrlJudulGroup2.Text = &quot;Department : &quot;
        ElseIf (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 2) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 9) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 16) Then
         qrlJudulReport2.Text = &quot;Employee Under Contract Report&quot;
         qrlSortBy2.Text = &quot;Report Type : Under Contract Per Job Title&quot;
         qrlJudulGroup2.Text = &quot;Job Title : &quot;
        ElseIf (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 3) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 10) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 17) Then
         qrlJudulReport2.Text = &quot;Employee Under Contract Report&quot;
         qrlSortBy2.Text = &quot;Report Type : Under Contract Per Job Level&quot;
         qrlJudulGroup2.Text = &quot;Job Level : &quot;
        ElseIf (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 4) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 11) or (CType(Report.GetColumnValue(&quot;DS.FgReport&quot;), Int32) = 18) Then
         qrlJudulReport2.Text = &quot;Employee Under Contract Report&quot;
         qrlSortBy2.Text = &quot;Report Type : Under Contract Per Employee Type&quot;
         qrlJudulGroup2.Text = &quot;Employee Type : &quot;
        End If
      End If
    End Sub
  End Class
End Namespace
