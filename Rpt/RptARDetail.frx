  Dim TotSaldo, Saldo as Decimal
  
    Dim UserCode as String
    Dim Nomor As Integer = 0 
    

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      TotSaldo = 0 
      UserCode = &quot;&quot;
      Nomor = Nomor + 1
      'Kredit = 0 
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Saldo = CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)
      TotSaldo = TotSaldo + CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal) - CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)      
      'Debet = Debet + CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal)
      'Kredit = Kredit + CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)
      Text28.Text = FormatNumber(Saldo,0)
      IF CType(Report.GetColumnValue(&quot;DS.UserCode&quot;), String)    = UserCode Then 
        Text1.Visible=False 
        Text4.Visible=False
        Text3.Visible=False
      Else 
        Text1.Visible=True 
        Text4.Visible=True 
        Text3.Visible=True
      End IF
      Text3.Text = Nomor.ToString 
    End Sub

    

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Text44.Text = FormatNumber(TotSaldo,0)
       UserCode=  CType(Report.GetColumnValue(&quot;DS.UserCode&quot;), String)
    End Sub
  End Class
End Namespace
</ScriptText>
