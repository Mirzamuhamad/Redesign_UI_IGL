    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      IF CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;1&quot;  Then
         Text15.Text = &quot;Januari&quot;
      ELSE IF CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;2&quot;  Then
        Text15.Text = &quot;Februari&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;3&quot;  Then
        Text15.Text = &quot;Maret&quot;
        ELSE IF CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;4&quot;  Then
        Text15.Text = &quot;April&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;5&quot;  Then
        Text15.Text = &quot;Mei&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;6&quot;  Then
        Text15.Text = &quot;Juni&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;7&quot;  Then
        Text15.Text = &quot;Juli&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;8&quot;  Then
        Text15.Text = &quot;Agustus&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;9&quot;  Then
        Text15.Text = &quot;September&quot;
       ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;10&quot;  Then
        Text15.Text = &quot;Oktober&quot;
      ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;11&quot;  Then
        Text15.Text = &quot;November&quot;
      ELSE IF  CType(Report.GetColumnValue(&quot;DS.Period&quot;), String)= &quot;12&quot;  Then
        Text15.Text = &quot;Desember&quot;
      END IF
      
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = nomor + 1
      Text18.Text = nomor.tostring
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = 0
    End Sub

    
