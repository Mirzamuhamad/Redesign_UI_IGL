    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)   
      If CType(Report.GetColumnValue(&quot;DS.StrDept&quot;), String) = &quot;Y&quot; Then
        'tDepartment.Visible = True    
        tDepartment.Text = &quot;Deaprtment : &quot; + CType(Report.GetColumnValue(&quot;DS.Department&quot;), String)
        GroupHeader1.StartNewPage = True
        GroupHeader1.Condition = &quot;[DS.Department]&quot;
      Else
        'tDepartment.Visible = False
        tDepartment.Text = &quot;Deaprtment : All&quot; 
        GroupHeader1.StartNewPage = False
        GroupHeader1.Condition = &quot;[DS.CompanyName]&quot;
      End If
    End Sub
