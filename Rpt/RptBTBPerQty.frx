  Public Class ReportScript
    dim supp as String
    dim No as Integer
    
    Private Sub ReportTitle1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
        supp = &quot;&quot;
        No = 0
        if Report.GetColumnValue(&quot;DS.FgForce&quot;) = &quot;Y&quot; then
          GroupHeader1.StartNewPage = true
        end if
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      supp = &quot;&quot;
    End Sub

    Private Sub GroupHeader2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      supp = &quot;&quot;
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      if supp &lt;&gt; Report.GetColumnValue(&quot;DS.SuppCode&quot;) then
        supp = Report.GetColumnValue(&quot;DS.SuppCode&quot;)
        No = No + 1
        txtNo.Text = No.ToString
        txtSupp.Visible=true
        txtSuppName.Visible = true
        txtNo.Visible = true
      else
        txtSupp.Visible=false
        txtSuppName.Visible = false
        txtNo.Visible = false
      end if
    End Sub
  End Class
End Namespace
