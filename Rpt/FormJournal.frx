    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Text6.Text = &quot;Remarks / Catatan : &quot;+ CType(Report.GetColumnValue(&quot;DS.Remark&quot;), String)
      Text13.Text = CType(Report.GetColumnValue(&quot;DS.Debit&quot;), String)
      Text14.Text = CType(Report.GetColumnValue(&quot;DS.Credit&quot;), String)
      Text15.Text = &quot;Say / Terbilang : &quot;+CType(Report.GetColumnValue(&quot;DS.Terbilang&quot;), String)+&quot; RUPIAH&quot;
      Text24.Text = CType(Report.GetColumnValue(&quot;DS.UserAppr&quot;), String)
      Text25.Text = CType(Report.GetColumnValue(&quot;DS.UserPrep&quot;), String)
     ' Text34.Text = CType(Report.GetColumnValue(&quot;DS.CompanyAddress&quot;), String)
    End Sub

    Private Sub PageHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      IF CType(Report.GetColumnValue(&quot;DS.TransClass&quot;), String)= &quot;JE&quot;  Then
         Text3.Text = &quot;JOURNAL ENTRY&quot;
      ELSE Text3.Text = CType(Report.GetColumnValue(&quot;DS.Trans_Type_Name&quot;), String)
      END IF
      
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = nomor + 1
      Text18.Text = nomor.tostring
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = 0
    End Sub

    

    Private Sub PageHeader1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      Picture1.Visible=true
    End Sub

    Private Sub GroupFooter1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      Picture1.Visible=false
      Shape1.Visible=False
    End Sub
  End Class
End Namespace
