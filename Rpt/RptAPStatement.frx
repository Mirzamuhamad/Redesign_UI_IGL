      Debet = Debet + CType(Report.GetColumnValue(&quot;DS.AmountDebit&quot;), Decimal)
      Kredit = Kredit + CType(Report.GetColumnValue(&quot;DS.AmountCredit&quot;), Decimal)
      Text43.Text = FormatNumber(Saldo,2)
    End Sub

    Private Sub GroupFooter1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Text44.Text = FormatNumber(Debet,2)
      Text45.Text = FormatNumber(Kredit,2)
      Text46.Text = FormatNumber(Saldo,2)      
    End Sub
  End Class
