      Kredit = 0  
      Saldo = CType(Report.GetColumnValue(&quot;DS.TotalBegin&quot;), Decimal) 
    End Sub
    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
