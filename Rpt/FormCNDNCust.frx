    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = nomor + 1
      Text79.Text = nomor.tostring
    End Sub

    Private Sub ReportTitle1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
     
    End Sub

    Private Sub Data1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      'txtterbilang.Text = UCase(CType(Report.GetColumnValue(&quot;DS.Terbilang&quot;), String))
      'txtDebit.Text= FormatNumber(CType(Report.GetTotalValue(&quot;TotDebit&quot;)), Decimal)
      'txtcredit .Text= FormatNumber(CType(Report.GetTotalValue(&quot;TotCredit&quot;)), Decimal)
      'text44.Text = CType(Report.GetColumnValue(&quot;DS.Currency&quot;), String)
    End Sub

    Private Sub GroupHeader1_AfterData(ByVal sender As object, ByVal e As EventArgs)
        Debit = 0
        credit = 0
    End Sub

    Private Sub GroupHeader1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Debit = Debit +  CType(Report.GetColumnValue(&quot;DS.Debit&quot;), Double)
      Credit = Credit +  CType(Report.GetColumnValue(&quot;DS.Credit&quot;), Double)
      'txtDebit.Text = FormatNumber(Debit,2) 
      'txtcredit.Text = FormatNumber(credit,2) 
      txtDebit.Text = FormatNumber(CType(Report.GetColumnValue(&quot;DS.TDebit&quot;), Double),2) 
      txtcredit.Text = FormatNumber(CType(Report.GetColumnValue(&quot;DS.TCredit&quot;), Double),2) 
      Text86.Text = CType(Report.GetColumnValue(&quot;DS.Terbilang&quot;), String)
      Text24.Text = CType(Report.GetColumnValue(&quot;DS.UserAppr&quot;), String)
      Text25.Text = CType(Report.GetColumnValue(&quot;DS.UserPrep&quot;), String)
    End Sub

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      nomor = 0
    End Sub

    Private Sub GroupFooter1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      Shape1.Visible=False
    End Sub

    Private Sub GroupFooter1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Shape1.Visible=True
    End Sub
