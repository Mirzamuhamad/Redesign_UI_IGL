    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      
      Count = Count + 1
      
      If CType(Report.GetColumnValue(&quot;DS.ProductCode&quot;), String) = Product Then
        Text9.Visible = False
