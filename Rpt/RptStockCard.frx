    Private Sub GroupHeader2_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      QtyActBalance = 0
    End Sub

    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      QtyActBalance = QtyActBalance + (CType(Report.GetColumnValue(&quot;DS.QtyIn&quot;), Double) - CType(Report.GetColumnValue(&quot;DS.QtyOut&quot;), Double))
      Text11.Text = FormatNumber(QtyActBalance,2)
      If CType(Report.GetColumnValue(&quot;DS.Reference&quot;), String) = &quot;OPENING&quot; Then
        Text1.Visible = False
      Else Text1.Visible = True
      End if  
      'Kolom = CType(Report.GetColumnValue(&quot;DS.ProductCode&quot;), String) 
    End Sub

    Private Sub ColumnHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      ColumnHeader1.Visible= Kolom = CType(Report.GetColumnValue(&quot;DS.ProductCode&quot;), String) 
    End Sub


    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      Kolom = &quot;&quot;
    End Sub

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      Kolom = CType(Report.GetColumnValue(&quot;DS.ProductCode&quot;), String) 
    End Sub
  End Class
End Namespace
