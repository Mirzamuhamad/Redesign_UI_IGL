      Dim OD1, OD2, OD3, OD4 AS Decimal
      If CType(Report.GetColumnValue(&quot;DS.FgBack&quot;), Int32) = 1 Then
        Text24.Text = &quot;Backward&quot;
      Else Text24.Text = &quot;Forward&quot;
      End If
      OD1 = CType(Report.GetColumnValue(&quot;DS.Param1&quot;), Int32) + 1
      OD2 = CType(Report.GetColumnValue(&quot;DS.Param2&quot;), Int32) + 1
      OD3 = CType(Report.GetColumnValue(&quot;DS.Param3&quot;), Int32) + 1
      OD4 = CType(Report.GetColumnValue(&quot;DS.Param4&quot;), Int32) + 1
      Text16.Text = OD1.ToString +&quot; - &quot;+CType(Report.GetColumnValue(&quot;DS.Param2&quot;), Int32).ToString+&quot; DAYS&quot; 
      Text18.Text = OD2.ToString +&quot; - &quot;+CType(Report.GetColumnValue(&quot;DS.Param3&quot;), Int32).ToString+&quot; DAYS&quot; 
      Text20.Text = OD3.ToString +&quot; - &quot;+CType(Report.GetColumnValue(&quot;DS.Param4&quot;), Int32).ToString+&quot; DAYS&quot;        
      Text27.Text = &quot; &gt; &quot;+OD4.ToString+&quot; DAYS&quot;        
    End Sub
  End Class
