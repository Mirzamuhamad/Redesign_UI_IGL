    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      SaldoRumah = SaldoRumah + CType(Report.GetColumnValue(&quot;DS.BalanceHome&quot;), Decimal) 
      SaldoForex = SaldoForex + CType(Report.GetColumnValue(&quot;DS.Amount&quot;), Decimal)   
      'SaldoRumah = SaldoRumah + CType(Report.GetColumnValue(&quot;DS.Amount&quot;), Decimal)
      Text53.Text = FormatNumber(SaldoRumah,2) 
      FixedCurr = CType(Report.GetColumnValue(&quot;DS.FixedCurrency&quot;), String)
    End Sub    

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
