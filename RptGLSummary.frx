    dim SaldoRumah as Double
    Dim TAccount as String

    Private Sub GroupHeader1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      if TAccount &lt;&gt; CType(Report.GetColumnValue(&quot;DS.AcctCode&quot;), String) then
        SaldoRumah = CType(Report.GetColumnValue(&quot;DS.SaldoHome&quot;), Decimal)
        Text12.Text = FormatNumber(SaldoRumah,2)
      else
        Text12.Text = FormatNumber(SaldoRumah-CType(Report.GetColumnValue(&quot;DS.BalanceHome&quot;),Decimal),2)
      End if  
      
    End Sub
          
    Private Sub Data1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      SaldoRumah = SaldoRumah + CType(Report.GetColumnValue(&quot;DS.BalanceHome&quot;), Decimal) 
      Text53.Text = FormatNumber(SaldoRumah,2)      
    End Sub    

    Private Sub Data1_AfterData(ByVal sender As object, ByVal e As EventArgs)
      text47.Visible = text46.Text &lt;&gt; &quot;&quot;
    End Sub

    Private Sub GroupHeader1_AfterPrint(ByVal sender As object, ByVal e As EventArgs)
      TAccount = CType(Report.GetColumnValue(&quot;DS.AcctCode&quot;), String) 
    End Sub
  End Class
End Namespace
